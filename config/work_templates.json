  "validation_schemas": {
    "workpiece_data": {
      "required_fields": ["part_type", "weight", "dimensions"],
      "optional_fields": ["material", "grip_points", "special_handling"]
    },
    "station_layout": {
      "required_fields": ["pick_stations", "place_stations"],
      "optional_fields": ["intermediate_positions", "quality_check_stations"]
    },
    "cycle_parameters": {
      "required_fields": ["cycle_time_target", "approach_speed", "work_speed"],
      "optional_fields": ["safety_height", "grip_force", "precision_mode"]
    }
